<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mobile.admdf">

    <application
        android:name=".AdmdfApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_admdf"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_admdf"
        android:supportsRtl="true"
        android:theme="@style/Theme.NotTitleBar">
        <!-- Activity for login navigation -->
        <activity android:name=".ui.activity.MainActivity" />

        <!-- Ativity for main navigation -->

        <activity android:name=".ui.activity.AuthActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>